  "private": true,
  "scripts": {
    "build": "webpack --config web.config.js",
    "start:dev": "webpack serve --config apps/server/webpack.config.js --mode development --hot",
    "dev:server": "npx nx run server:serve:development --skip-nx-cache ",
    "dev:client": "npx nx run client:serve:development",
    "prisma:studio": "cd libs/nestjs-libraries/src/database && npx prisma studio",
    "prisma:generate": "cd libs/nestjs-libraries/src/database && npx prisma generate",
    "prisma:migrate": "cd libs/nestjs-libraries/src/database && npx prisma migrate dev",
    "prisma:dbpush": " cd libs/nestjs-libraries/src/database && npx prisma db push",
    "prisma:dbpull": " cd libs/nestjs-libraries/src/database && npx prisma db pull",
    "prisma:seed": "npx prisma db seed",
    "clear": "rm -rfv assets/localStorage/*",
    "nx:build": "npx nx run-many --target=build"
  },
  "prisma": {
    "seed": "ts-node libs/nestjs-libraries/src/database/prisma/seed.ts"
  },
